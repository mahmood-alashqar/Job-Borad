<br>
<div class=".container-fluid">
<% @job.each do |s| %>
    <%= form_with model:@job, url:  jobs_Applied_jobs_path do |form| %>
    <div class="alert alert-success" role="alert" >
  <h4 class="alert-heading"><%= s.title %></h4>
  <hr>
  <p style="margin-left:auto; margin-right:auto; width:60%; height: 200px; overflow-y: scroll;"> <%= s.description %> </p>
  <hr>
  <p class="mb-2"><%= s.created_at %> </p><%= form.submit "Apply", class:"btn btn-primary" %>
</div>
<% end %>
<% end %>
</div>